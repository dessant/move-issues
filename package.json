{
  "name": "move-issues",
  "version": "0.4.0",
  "description": "A GitHub App built with Probot that moves issues between repositories.",
  "author": "Armin Sebastian",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/dessant/move-issues.git",
    "type": "git"
  },
  "bugs": "https://github.com/dessant/move-issues/issues",
  "main": "src/index.js",
  "scripts": {
    "start": "probot run ./src/index.js",
    "watch": "nodemon --exec 'yarn run start'",
    "update": "ncu --upgrade --upgradeAll && yarn",
    "push": "git push --follow-tags origin master",
    "release": "standard-version && yarn run push"
  },
  "files": [
    "docs",
    "src"
  ],
  "dependencies": {
    "joi": "^13.4.0",
    "moment": "^2.22.2",
    "probot": "^6.2.0",
    "probot-commands": "1.1.0",
    "probot-config": "^0.1.0",
    "probot-messages": "^0.1.0",
    "to-markdown": "^3.1.1",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "nodemon": "1.17.5",
    "npm-check-updates": "^2.14.2",
    "smee-client": "^1.0.1",
    "standard-version": "^4.4.0"
  },
  "engines": {
    "node": "8.9.0",
    "npm": "5.5.1"
  },
  "keywords": [
    "github",
    "issues",
    "pull requests",
    "move",
    "copy",
    "move issues",
    "copy issues",
    "automation",
    "github app",
    "probot",
    "probot app",
    "bot"
  ]
}
